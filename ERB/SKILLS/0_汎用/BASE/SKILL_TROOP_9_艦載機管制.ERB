@SKILL_0_TROOP_9_EXIST
RETURN 1

@SKILL_0_TROOP_9_NAME
RESULTS = 艦載機管制

;レベルは1-5まで
@SKILL_0_TROOP_9_LEVEL
RETURN 3

@SKILL_0_TROOP_9_EXPLANATION
RESULTS = 艦隊の対空命中率と艦載機命中率+10\%(必要知略50)


@SKILL_0_TROOP_9_EFFECT(VALUE, TYPE, CLASS, CH_ID)
#DIM VALUE
#DIM TYPE
#DIM CLASS
#DIM CH_ID

LOCAL = ABL:CH_ID:知略
	IF LOCAL >= 50
		IF TYPE == TYPE_命中 && CLASS == 艦種_航空機
			TIMES VALUE, 1.10
		ELSEIF TYPE == TYPE_対空命中
			TIMES VALUE, 1.10
		ENDIF
	ELSE
		IF TYPE == TYPE_命中 && CLASS == 艦種_航空機
			TIMES VALUE, 1.05
		ELSEIF TYPE == TYPE_対空命中
			TIMES VALUE, 0.95
		ENDIF
	ENDIF
RETURN VALUE

@SKILL_0_TROOP_9_RATE(発動者, 発動番号, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
#DIM 発動者
#DIM 発動番号
#DIM スキル
#DIMS ジャンル
#DIM 発動側
#DIM 発動勢力
#DIM 発動部隊
#DIM 対象側

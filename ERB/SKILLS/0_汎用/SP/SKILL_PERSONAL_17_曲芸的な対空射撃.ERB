@SKILL_0_PERSONAL_17_EXIST
RETURN 1

@SKILL_0_PERSONAL_17_NAME
RESULTS = 曲芸的な対空射撃

;レベルは1-5まで
@SKILL_0_PERSONAL_17_LEVEL
RETURN 3

@SKILL_0_PERSONAL_17_EXPLANATION
RESULTS = 自艦の対空命中率と対空ダメージ+30\%(必要操艦80)

;効果をここに記述
@SKILL_0_PERSONAL_17_EFFECT(VALUE, TYPE, CLASS, CHARA_ID)
#DIM VALUE
#DIM TYPE
#DIM CLASS
#DIM CHARA_ID

LOCAL = ABL:CHARA_ID:操艦
	IF LOCAL >= 40
		IF TYPE == TYPE_対空命中
			TIMES VALUE, 1.30
		ELSEIF TYPE == TYPE_対空ダメージ
			TIMES VALUE, 1.30
		ENDIF
	ELSE
		IF TYPE == TYPE_対空命中
			TIMES VALUE, 1.02
		ELSEIF TYPE == TYPE_対空ダメージ
			TIMES VALUE, 1.02
		ENDIF
	ENDIF
RETURN VALUE

@SKILL_0_PERSONAL_17_RATE(発動者, 発動番号, 発動側, 発動勢力, 発動部隊, 対象勢力, 対象部隊)
#DIM 発動者
#DIM 発動番号
#DIM スキル
#DIMS ジャンル
#DIM 発動側
#DIM 発動勢力
#DIM 発動部隊
#DIM 対象側
#DIM 対象勢力

@SKILL_0_PERSONAL_17_EFFECT(TARGET_ID, CAPTAIN)
#DIM TARGET_ID
#DIM CAPTAIN

LOCAL = ABL:CAPTAIN:操艦
IF LOCAL >= 80
	TIMES SKILL_EFFECT_PERCENTAGE:TYPE_対空命中:TARGET_ID, 1.20
	TIMES SKILL_EFFECT_PERCENTAGE:TYPE_対空ダメージ:TARGET_ID, 1.20
ELSE
	TIMES SKILL_EFFECT_PERCENTAGE:TYPE_対空命中:TARGET_ID, 1.10
	TIMES SKILL_EFFECT_PERCENTAGE:TYPE_対空ダメージ:TARGET_ID, 1.10
ENDIF


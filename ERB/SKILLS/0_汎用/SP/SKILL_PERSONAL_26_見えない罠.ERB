@SKILL_0_PERSONAL_26_EFFECT(VALUE, TYPE, CLASS, CHARA_ID)
#DIM VALUE
#DIM TYPE
#DIM CLASS
#DIM CHARA_ID

LOCAL = ABL:CHARA_ID:知略
	IF LOCAL >= 50
		SIF TYPE == TYPE_敵回避
			TIMES VALUE, 0.85
	ELSE
		SIF TYPE == TYPE_敵回避
			TIMES VALUE, 0.97
	ENDIF
RETURN VALUE





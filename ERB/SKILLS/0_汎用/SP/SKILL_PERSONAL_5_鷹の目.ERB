@SKILL_0_PERSONAL_5_EFFECT(VALUE, TYPE, CLASS, CHARA_ID)
#DIM VALUE
#DIM TYPE
#DIM CLASS
#DIM CHARA_ID

LOCAL = ABL:CHARA_ID:操艦
	IF LOCAL >= 60
		SIF TYPE == TYPE_命中
			TIMES VALUE, 1.25
	ELSE
		SIF TYPE == TYPE_命中
			TIMES VALUE, 1.02
	ENDIF
RETURN VALUE



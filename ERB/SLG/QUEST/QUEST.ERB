;--------------------------
; クエストリスト
;--------------------------
@QUEST_LIST
#DIM ITER
FOR ITER, 0, QUEST_LENGTH
	RESULT = 0
	TRYCALLFORM QUEST_ACCEPTABLE_{ITER}
	IF RESULT == 1
		RESULTS = 
		TRYCALLFORM QUEST_NAME_{ITER}
		PRINTBUTTON @"[{ITER}]%RESULTS%", ITER
		PRINTL
	ENDIF
NEXT

PRINTBUTTON "[999]戻る", 999
INPUT
IF RESULT >= 0 && RESULT < QUEST_LENGTH
	TRYCALLFORM QUEST_BODY_{RESULT}
ENDIF


;-----------------------------------
; エンジン
;-----------------------------------
@GET_ENGIN_NAME_11
RESULTS = TIER3大型戦闘用エンジン

@GET_ENGIN_STATUS_11(STATUS)
#DIM REF STATUS

STATUS:SPEED = 1
STATUS:AVOID = 15
STATUS:SIZE = 3
STATUS:PRICE = 160000

;-----------------------------------
; エンジンの説明
;-----------------------------------
@GET_ENGIN_DESCRIPTION_11(STATUS)
#DIM STATUS, STATUS_LENGTH

CALL GET_ENGIN_STATUS_11(STATUS)
CALL GET_ENGIN_NAME_11
PRINTFORML %RESULTS%
PRINTFORML スピード  : {STATUS:SPEED}
PRINTFORML 回避      : {STATUS:AVOID}
PRINTFORML サイズ    : {STATUS:SIZE}

;-----------------------------------
; エンジンの資材
;-----------------------------------
@GET_ENGIN_RESOURCE_11(USE_RESOURCE)
#DIM REF USE_RESOURCE

USE_RESOURCE:耐熱合金 = 32
USE_RESOURCE:ジェットガス = 32

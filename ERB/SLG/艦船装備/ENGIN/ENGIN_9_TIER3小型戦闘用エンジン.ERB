STATUS:SPEED = 1
STATUS:AVOID = 15
STATUS:SIZE = 1
STATUS:PRICE = 40000

;-----------------------------------
; エンジンの説明
;-----------------------------------
@GET_ENGIN_DESCRIPTION_9(STATUS)

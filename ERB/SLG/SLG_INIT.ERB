VARSET BOARDING_SHIP_ID, -1
VARSET RIDING_WITH, -1
VARSET PORT_POSITION_ID, -1
VARSET MASTER_LEFT_DAY, -1
VARSET MINING_POINT, -1

PRINTL 各勢力の初期化中
;命名
COUNTRY_NAME:0 = ERROR

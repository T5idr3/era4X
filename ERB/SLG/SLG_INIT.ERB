@INIT_COUNTRY_10
#DIM CONST X座標分割数 = MAP_WIDTH / 5
#DIM CONST Y座標分割数 = MAP_HEIGHT / 5
#DIM 星系座標X, 星系数
#DIM 星系座標Y, 星系数
#DIM ITER
#DIM SECTOR_ID
#DIM X_POS
#DIM Y_POS

ITER = 0
COUNTRY_NAME:10 = 無所属

;関係性
FOR LOCAL, 1, MAX_COUNTRY
	SIF LOCAL == 10
		CONTINUE
	REL_LIKE:10:LOCAL = -1000
	REL_LIKE:LOCAL:10 = -1000
NEXT

WHILE ITER < 50
	FOR SECTOR_ID, 0, MAX_SECTOR
		FOR Y_POS, 0, Y座標分割数 - 1
			FOR X_POS, 0, X座標分割数 - 1
				IF ITER < 50 && RAND(50 - ITER) == 0 && MAP:SECTOR_ID:(X_POS * 5 + 3):(Y_POS * 5 + 3) != 0
					CALL POS2ID(SECTOR_ID, 1, 1)
					;LOCALS = 海賊船＃{ITER}
					CALL INIT_PRESET_SHIP(10, RESULT, 汎用コルベット, , 汎用コルベット)
					ITER ++
				ENDIF
			NEXT
		NEXT
	NEXT
WEND
ITER = 0
WHILE ITER < 50
	FOR SECTOR_ID, 0, MAX_SECTOR
		FOR Y_POS, 0, Y座標分割数 - 1
			FOR X_POS, 0, X座標分割数 - 1
				IF ITER < 50 && RAND(50 - ITER) == 0 && MAP:SECTOR_ID:(X_POS * 5 + 3):(Y_POS * 5 + 3) != 0
					CALL POS2ID(SECTOR_ID, 9, 9)
					LOCALS = 宇宙巨大スライム
					CALL INIT_PRESET_SHIP(10, RESULT, 宇宙巨大スライム, LOCALS, 宇宙巨大スライム)
					ITER ++
				ENDIF
			NEXT
		NEXT
	NEXT
WEND


;-------------------------------------------------
;二本フェラ(奴隷に二本舐めさせる)
;奉仕、レズ系派生コマンド
;-------------------------------------------------
@COM511
PRINTL 二本フェラ
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;共通処理
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0
	


;-------------------------------------------------
;実行決定
;-------------------------------------------------
STR:0 = 二本フェラ
SELECTCOM = 511
CALL KOJO_MESSAGE_COM

;実行者が奴隷であるフラグ
TFLAG:28 = 1

;-------------------------------------------------
;射精ゲージチェック（主人）
;-------------------------------------------------
B = 750

;ABL:調教者の技巧をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSEIF ABL:2 == 3
	TIMES B , 1.60
ELSEIF ABL:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:奉仕精神をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;TALENT:舌使い、猫舌をみる
SIF TALENT:舌使い || TALENT:猫舌
	TIMES B , 2.00

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:MASTER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:MASTER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:MASTER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:MASTER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:MASTER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:MASTER:半人半妖 || TALENT:MASTER:ふたなり  || TALENT:MASTER:オトコ
	BASE:MASTER:2 += B


;-------------------------------------------------
;射精ゲージチェック（助手）
;-------------------------------------------------
B = 750

;ABL:調教者の技巧をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSEIF ABL:2 == 3
	TIMES B , 1.60
ELSEIF ABL:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:奉仕精神をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;TALENT:舌使い、猫舌をみる
SIF TALENT:舌使い || TALENT:猫舌
	TIMES B , 2.00

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:ASSI:3 == 0
	TIMES B , 1.00
ELSEIF ABL:ASSI:3 == 1
	TIMES B , 1.50
ELSEIF ABL:ASSI:3 == 2
	TIMES B , 2.00
ELSEIF ABL:ASSI:3 == 3
	TIMES B , 2.50
ELSEIF ABL:ASSI:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:ASSI:半人半妖 || TALENT:ASSI:ふたなり  || TALENT:ASSI:オトコ
	BASE:ASSI:2 += B


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:0 += 80
LOSEBASE:1 += 250

SOURCE:4 = 150
SOURCE:5 = 400
SOURCE:6 = 300
SOURCE:14 = 1000
SOURCE:17 = 500
SOURCE:18 = 500


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:16 += 700

;-------------------------------------------------
;射精チェック（主人）
;-------------------------------------------------
S = BASE:MASTER:2
EJAC = MAXBASE:MASTER:2

;射精限界が来ている場合は、射精ゲージは強制的に0にする
IF CFLAG:MASTER:11 == 0
	S = 0
	BASE:MASTER:2 = 0
ENDIF

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

;射精時に中毒充足のソースを追加
;通常射精時
SIF E == 1
	A = 200
;大量射精時
SIF E == 2
	A = 500

IF E > 0
	;精液中毒を見る
	IF ABL:16 == 1
		TIMES A , 0.00
	ELSEIF ABL:16 == 2
		TIMES A , 0.30
	ELSEIF ABL:16 == 3
		TIMES A , 0.60
	ELSEIF ABL:16 == 4
		TIMES A , 0.90
	ELSEIF ABL:16 == 5
		TIMES A , 1.20
	ELSEIF ABL:16 == 6
		TIMES A , 1.50
	ELSEIF ABL:16 == 7
		TIMES A , 1.80
	ELSEIF ABL:16 == 8
		TIMES A , 2.30
	ELSEIF ABL:16 == 9
		TIMES A , 3.00
	ELSEIF ABL:16 >= 10
		TIMES A , 5.00
	ENDIF
ENDIF

SOURCE:9 += A

;射精後は再装填の時間を設ける
;通常射精時
SIF E == 1
	CFLAG:MASTER:24 = 3
;大量射精時
SIF E == 2
	CFLAG:MASTER:24 = 5

;大量射精
IF E == 2

	EXP:MASTER:射精経験 += 2
	EXP:精液経験 += 9
	PRINTL 大量射精
	PRINTL 精液経験＋９
	;精液汚れ
	STAIN:MASTER:2 |= 4

	BASE:MASTER:2 -= EJAC*2
	SIF BASE:MASTER:2 >= EJAC
		BASE:MASTER:2 = EJAC-1

	;口で射精させたフラグ
	TFLAG:0 = 2
;通常の射精
ELSEIF E == 1
	EXP:MASTER:射精経験 += 1
	EXP:精液経験 += 3
	PRINTL 射精
	PRINTL 精液経験＋３
	;精液汚れ
	STAIN:MASTER:2 |= 4

	BASE:MASTER:2 -= EJAC
	SIF BASE:MASTER:2 >= EJAC
		BASE:MASTER:2 = EJAC-1

	;口で射精させたフラグ
	TFLAG:0 = 1
ENDIF


;-------------------------------------------------
;射精チェック（助手）
;-------------------------------------------------
S = BASE:ASSI:2
EJAC = MAXBASE:ASSI:2

;射精限界が来ている場合は、射精ゲージは強制的に0にする
IF CFLAG:ASSI:11 == 0
	S = 0
	BASE:ASSI:2 = 0
ENDIF

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

;射精時に中毒充足のソースを追加
;通常射精時
SIF E == 1
	A = 200
;大量射精時
SIF E == 2
	A = 500

IF E > 0
	;精液中毒を見る
	IF ABL:16 == 1
		TIMES A , 0.00
	ELSEIF ABL:16 == 2
		TIMES A , 0.30
	ELSEIF ABL:16 == 3
		TIMES A , 0.60
	ELSEIF ABL:16 == 4
		TIMES A , 0.90
	ELSEIF ABL:16 == 5
		TIMES A , 1.20
	ELSEIF ABL:16 == 6
		TIMES A , 1.50
	ELSEIF ABL:16 == 7
		TIMES A , 1.80
	ELSEIF ABL:16 == 8
		TIMES A , 2.30
	ELSEIF ABL:16 == 9
		TIMES A , 3.00
	ELSEIF ABL:16 >= 10
		TIMES A , 5.00
	ENDIF
ENDIF

SOURCE:9 += A

;射精後は再装填の時間を設ける
;通常射精時
SIF E == 1
	CFLAG:ASSI:24 = 3
;大量射精時
SIF E == 2
	CFLAG:ASSI:24 = 5

;大量射精
IF E == 2
	EXP:ASSI:射精経験 += 2
	EXP:精液経験 += 3
	PRINTL 大量射精（助手）
	PRINTL 精液経験＋３
	;Ｐに精液汚れ
	STAIN:ASSI:2 |= 4

	BASE:ASSI:2 -= EJAC*2
	SIF BASE:ASSI:2 >= EJAC
		BASE:ASSI:2 = EJAC-1
	;助手が射精フラグ
	TFLAG:6 = 2
;通常の射精
ELSEIF E == 1
	EXP:ASSI:射精経験 += 1
	EXP:精液経験 += 1
	PRINTL 射精（助手）
	PRINTL 精液経験＋１
	;Ｐに精液汚れ

	STAIN:ASSI:2 |= 4

	BASE:ASSI:2 -= EJAC
	SIF BASE:ASSI:2 >= EJAC
		BASE:ASSI:2 = EJAC-1
	;助手が射精フラグ
	TFLAG:6 = 1
ENDIF


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の口⇔主人のＰの汚れが移動
IF TALENT:MASTER:ふたなり || TALENT:MASTER:オトコ
	STAIN:0 |= STAIN:MASTER:2
	STAIN:MASTER:2 |= STAIN:0
ENDIF
;奴隷の口⇔助手のＰの汚れが移動
IF TALENT:ASSI:ふたなり || TALENT:ASSI:オトコ
	STAIN:0 |= STAIN:ASSI:2
	STAIN:ASSI:2 |= STAIN:0
ENDIF

;奉仕精神ＬＶ２以上、技巧ＬＶ２以上なら汚れをなめ取る（ボールギャグ使用時は除く）
IF ABL:7 >= 2 && ABL:2 >= 2 && TEQUIP:20 == 0
	STAIN:MASTER:2 = 2
	STAIN:ASSI:2 = 2
	SIF E >= 1
		TFLAG:8 = 1
ENDIF


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;フェラ経験
PRINTFORML %EXPNAME:22%＋２
EXP:フェラ経験 += 2

;レズ経験
IF TALENT:オトコ == 0 && TALENT:MASTER:オトコ == 0 && TALENT:ASSI:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +10
	EXP:レズ経験 += 10
	A = NO:MASTER + 899
	CFLAG:A += 5
	A = NO:ASSI + 899
	CFLAG:A += 5
ELSEIF TALENT:オトコ == 0 && TALENT:MASTER:オトコ == 0 && TALENT:ASSI:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +5
	EXP:ゲイ経験 += 5
	A = NO:MASTER + 899
	CFLAG:A += 5
ELSEIF TALENT:オトコ == 0 && TALENT:MASTER:オトコ == 1 && TALENT:ASSI:オトコ == 0
	PRINTS EXPNAME:41
	PRINTL +5
	EXP:ゲイ経験 += 5
	A = NO:ASSI + 899
	CFLAG:A += 5
ENDIF

;奴隷が猫舌持ちの場合は、調教者に嗜虐快楽経験を追加
IF TALENT:TARGET:猫舌
	EXP:嗜虐快楽経験 += 1
	PRINTFORML %EXPNAME:32%＋１
ENDIF

;依存度ベクトル
P = NO:PLAYER
IF RELATION:P > 100
	TFLAG:33 = 2
ELSEIF RELATION:P < 100
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 1
ENDIF

;依存度補正値(相性100↑かつ淫乱で2、娼婦で4）
P = NO:PLAYER
IF TALENT:淫乱 && RELATION:P > 100
	TFLAG:30 += 2
ELSEIF TALENT:娼婦 && RELATION:P > 100
	TFLAG:30 += 4
ENDIF

;奉仕快楽フラグ
TFLAG:100 = 1
RETURN 1

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_511
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	SIF S
		PRINT  + 
	A += ABL:1 * 1
	PRINTS ABLNAME:1
	PRINTV 'LV,ABL:1,'(,ABL:1 * 1,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:7
	SIF S
		PRINT  + 
	A += ABL:7 * 4
	PRINTS ABLNAME:7
	PRINTV 'LV,ABL:7,'(,ABL:7 * 4,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:16
	SIF S
		PRINT  + 
	A += ABL:16 * 3
	PRINTS ABLNAME:16
	PRINTV 'LV,ABL:16,'(,ABL:16 * 3,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 1
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 1,')
	S = 1
ENDIF


;PALAM:欲情
IF PALAM:6 < PALAMLV:1
	L = 0
ELSEIF PALAM:6 < PALAMLV:2
	L = 1
ELSEIF PALAM:6 < PALAMLV:3
	L = 2
ELSEIF PALAM:6 < PALAMLV:4
	L = 3
ELSEIF PALAM:6 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 1
	PRINTS PALAMNAME:6
	PRINTV 'LV,L,'(,L * 1,')
	S = 1
ENDIF

;恥じらい
IF TALENT:恥じらい
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:35
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭鈍感
IF TALENT:汚臭鈍感
	SIF S
		PRINT  + 
	A += 1
	PRINTS TALENTNAME:61
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭敏感
IF TALENT:汚臭敏感
	PRINT  - 
	A -= 3
	PRINTS TALENTNAME:62
	PRINTV '(,3,')
	S = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF S
		PRINT  + 
	A += 6
	PRINTS TALENTNAME:63
	PRINTV '(,6,')
	S = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感の否定 && (TALENT:淫乱 == 0 && TALENT:娼婦 == 0)
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:71
	PRINTV '(,1,')
	S = 1
ENDIF
;淫乱
IF TALENT:淫乱 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:4
	PRINTV '(,5,')
	S = 5
ENDIF
;娼婦
IF TALENT:娼婦 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 20
	PRINTS TALENTNAME:7
	PRINTV '(,10,')
	S = 10
ENDIF
;調教者がふたなり
IF TALENT:PLAYER:ふたなり
	SIF S
		PRINT  + 
	A += 8
	PRINTS TALENTNAME:139
	PRINTV '(,8,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15

SIF TALENT:汚臭鈍感
	Y /= 3
SIF TALENT:汚臭敏感
	Y *= 2



;汚れあり
IF Y
	PRINT  - 
	A -= Y
	;汚臭鈍感
	IF TALENT:汚臭鈍感
		PRINT 汚れあり、
		PRINTS TALENTNAME:61
	ELSEIF TALENT:汚臭敏感
		PRINT 汚れあり、
		PRINTS TALENTNAME:62
	ELSE
		PRINT 汚れあり
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(32以上で実行)
PRINT  = 
PRINTV A

V = 32
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0


RETURN 1


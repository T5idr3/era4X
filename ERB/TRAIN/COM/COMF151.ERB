;-------------------------------------------------
;鰻風呂
;ハードな調教コマンド、レベル4
;-------------------------------------------------
@COM151
PRINTL 鰻風呂
STR:0 = 鰻風呂
CALL KOJO_MESSAGE_COM

F = 1
R = 1
;Vに道具を挿入している場合
IF TEQUIP:13
	F = 0
ELSE
	;V経験を伴うコマンドのフラグ(処女膜再生対応)
	TFLAG:19 = 1
ENDIF

;Aに道具を挿入している場合
IF TEQUIP:14 || TEQUIP:15 || TEQUIP:21 || TEQUIP:22 || TEQUIP:23
	R = 0
ENDIF

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:0 += 400
LOSEBASE:1 += 650

SOURCE:0 = 100
SOURCE:3 = 100
SOURCE:7 = 1000
SOURCE:8 = 2000
SOURCE:14 = 1200
SOURCE:15 = 800
SOURCE:17 = 1500
SOURCE:18 = 1500
;Vに道具を挿入している場合
IF F > 0
	SOURCE:1 = 100
	SOURCE:7 += 500
	SOURCE:8 += 500
ENDIF
;Aに道具を挿入している場合
IF R > 0
	SOURCE:2 = 100
	SOURCE:7 += 500
	SOURCE:8 += 500
ENDIF

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;Vに道具を挿入している場合のみ見る
IF F > 0
	;Vへの苦痛は先に処理しておく
	;EXP:Ｖ経験をみる（処女の反感は別途処理済みなので省略）
	IF EXP:Ｖ経験 < EXPLV:1
		TIMES SOURCE:7 , 2.50
	ELSEIF EXP:Ｖ経験 < EXPLV:2
		TIMES SOURCE:7 , 0.80
		TIMES SOURCE:18 , 1.20
	ELSEIF EXP:Ｖ経験 < EXPLV:3
		TIMES SOURCE:7 , 0.40
	ELSEIF EXP:Ｖ経験 < EXPLV:4
		TIMES SOURCE:7 , 0.10
	ELSEIF EXP:Ｖ経験 < EXPLV:5
		TIMES SOURCE:7 , 0.00
	ELSEIF EXP:Ｖ経験 >= EXPLV:5
		TIMES SOURCE:7 , 0.00
	ENDIF

	;PALAM:潤滑をみる
	IF PALAM:4 < PALAMLV:1
		TIMES SOURCE:7 , 1.20
		TIMES SOURCE:18 , 1.50
	ELSEIF PALAM:4 < PALAMLV:2
		TIMES SOURCE:7 , 0.70
		TIMES SOURCE:18 , 1.20
	ELSEIF PALAM:4 < PALAMLV:3
		TIMES SOURCE:7 , 0.40
	ELSEIF PALAM:4 < PALAMLV:4
		TIMES SOURCE:7 , 0.10
	ELSEIF PALAM:4 >= PALAMLV:4
		TIMES SOURCE:7 , 0.10
	ENDIF
ENDIF

;Aに道具を挿入している場合のみ見る
IF R > 0
	;Aへの苦痛は先に処理しておく
	;EXP:Ａ経験をみる
	IF EXP:Ａ経験 < EXPLV:1
		TIMES SOURCE:7 , 3.00
		TIMES SOURCE:18 , 3.00
	ELSEIF EXP:Ａ経験 < EXPLV:2
		TIMES SOURCE:7 , 1.20
		TIMES SOURCE:18 , 2.00
	ELSEIF EXP:Ａ経験 < EXPLV:3
		TIMES SOURCE:7 , 0.80
		TIMES SOURCE:18 , 1.50
	ELSEIF EXP:Ａ経験 < EXPLV:4
		TIMES SOURCE:7 , 0.60
		TIMES SOURCE:18 , 1.20
	ELSEIF EXP:Ａ経験 < EXPLV:5
		TIMES SOURCE:7 , 0.40
	ELSEIF EXP:Ａ経験 >= EXPLV:5
		TIMES SOURCE:7 , 0.20
	ENDIF

	;PALAM:潤滑をみる
	IF PALAM:4 < PALAMLV:1
		TIMES SOURCE:7 , 1.20
		TIMES SOURCE:18 , 2.00
	ELSEIF PALAM:4 < PALAMLV:2
		TIMES SOURCE:7 , 1.00
		TIMES SOURCE:18 , 1.50
	ELSEIF PALAM:4 < PALAMLV:3
		TIMES SOURCE:7 , 0.60
	ELSEIF PALAM:4 < PALAMLV:4
		TIMES SOURCE:7 , 0.30
	ELSEIF PALAM:4 >= PALAMLV:4
		TIMES SOURCE:7 , 0.20
	ENDIF
ENDIF

;特にハードなコマンドでは、異常経験に応じて効果を減らす
IF EXP:異常経験 < EXPLV:1
	TIMES SOURCE:8 , 1.20
	TIMES SOURCE:17 , 1.20
	TIMES SOURCE:18 , 1.20
ELSEIF EXP:異常経験 < EXPLV:2
	TIMES SOURCE:8 , 0.90
	TIMES SOURCE:17 , 0.90
	TIMES SOURCE:18 , 0.90
ELSEIF EXP:異常経験 < EXPLV:3
	TIMES SOURCE:8 , 0.80
	TIMES SOURCE:17 , 0.80
	TIMES SOURCE:18 , 0.80
ELSEIF EXP:異常経験 < EXPLV:4
	TIMES SOURCE:8 , 0.70
	TIMES SOURCE:17 , 0.70
	TIMES SOURCE:18 , 0.70
ELSEIF EXP:異常経験 < EXPLV:5
	TIMES SOURCE:8 , 0.60
	TIMES SOURCE:17 , 0.60
	TIMES SOURCE:18 , 0.60
ELSE
	TIMES SOURCE:8 , 0.50
	TIMES SOURCE:17 , 0.50
	TIMES SOURCE:18 , 0.50
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;粘液の汚れ
STAIN:1 = 32
STAIN:2 = 32
STAIN:3 = 32
SIF R > 0
	STAIN:4 = 32
STAIN:5 = 32
IF F > 0
	STAIN:6 = 32
	;膣内に破瓜の血の汚れを付加
	SIF TALENT:処女 == 1 || TALENT:再生処女 == 1
		STAIN:6 |= 64
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;V経験
IF F > 0
	IF EXP:Ｖ経験 == 0
		EXP:異常経験 += 2
		PRINTL 異常経験＋２
	ENDIF
	EXP:Ｖ経験 += 2
	PRINTL Ｖ経験＋２
ENDIF

;A経験
IF R > 0
	EXP:Ａ経験 += 2
	PRINTL Ａ経験＋２
	STAIN:4 = 8
ENDIF

;異常経験が5未満のとき、異常経験追加
IF EXP:異常経験 < 5
	EXP:異常経験 += 2
	PRINTL 異常経験＋２
ENDIF

;依存度ベクトル
TFLAG:33 = 5

;依存度補正値(服従で1、隷属で2）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1


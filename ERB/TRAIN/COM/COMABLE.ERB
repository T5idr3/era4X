ELSEIF PALAM:4 < PALAMLV:2
	SIF (ABL:ASSI:0 <= 3 || ABL:ASSI:11 <= 3) && TALENT:ASSI:サド == 0
		RETURN 0
ENDIF
A = 0
;MASTERがオトコ、半人半妖、ふたなり
SIF TALENT:MASTER:半人半妖 || TALENT:MASTER:ふたなり || TALENT:MASTER:オトコ
	A += 1

;-------------------------------------------------
;胸揉み
;愛撫系コマンド、レベル1
;-------------------------------------------------
@COM6
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;母乳体質の場合搾乳に飛ぶ
SIF TALENT:母乳体質
	JUMP COM509

PRINTL 胸揉み
STR:0 = 胸揉み
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:0 += 5
LOSEBASE:1 += 30

SOURCE:3 = 80
SOURCE:4 = 20
SOURCE:18 = 10


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:16 += 10

;調教者が［幼稚］か［幼児退行］
IF TALENT:PLAYER:幼児／幼児退行 || TALENT:PLAYER:幼稚
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:4 , 1.20
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＢ⇔調教者の指の汚れが移動
	STAIN:5 |= STAIN:PLAYER:1
	STAIN:PLAYER:1 |= STAIN:5

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
A = 5
B = NO:PLAYER + 899
IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:レズ経験 += A
	CFLAG:B += A
ELSEIF TALENT:オトコ && TALENT:PLAYER:オトコ
	PRINTFORML %EXPNAME:41%+{A}
	EXP:ゲイ経験 += A
	CFLAG:B += A
ENDIF

;依存度ベクトル
TFLAG:33 = 2


RETURN 1


;-------------------------------------------------
;コンドーム精飲(助手)
;特殊コマンド
;-------------------------------------------------
@COM193
PRINTL コンドーム精飲(助手)
STR:0 = コンドーム精飲(助手)
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:0 += 0
LOSEBASE:1 += 30

SOURCE:4 = 100
SOURCE:10 = 100
SOURCE:14 = 800
SOURCE:17 = 200
SOURCE:18 = 200


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:16 += 50

;助手と奴隷の相性によって、全体のソースが変化
T = NO:TARGET
A = NO:ASSI
SOURCE:4 *= RELATION:A
SOURCE:10 *= RELATION:A
SOURCE:14 *= RELATION:A
SOURCE:16 *= RELATION:A
SOURCE:17 *= RELATION:A
SOURCE:18 *= RELATION:A
SOURCE:4 /= 100
SOURCE:10 /= 100
SOURCE:14 /= 100
SOURCE:16 /= 100
SOURCE:17 /= 100
SOURCE:18 /= 100

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;助手の口に精液汚れ
STAIN:ASSI:0 |= 4

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;コンドームの精液を初期化
CFLAG:107 = 0


RETURN 1


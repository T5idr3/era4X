;-------------------------------------------------
;乳スパンキング
;SM系コマンド、レベル1
;-------------------------------------------------
@COM101
PRINTL 乳スパンキング
STR:0 = 乳スパンキング
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:0 += 50
LOSEBASE:1 += 30

SOURCE:7 = 450
SOURCE:8 = 400
SOURCE:17 = 50
SOURCE:18 = 300


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;マゾっ気に応じて快Bのソースを追加する
SOURCE:3 += 100*ABL:9


;苦痛のパラメータによる苦痛のソース増加は先に処理しておく
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:7 , 1.00
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:7 , 1.50
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:7 , 2.00
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:7 , 2.50
ELSEIF PALAM:10 >= PALAMLV:4
	TIMES SOURCE:7 , 3.00
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＢ⇔調教者の指の汚れが移動
STAIN:5 |= STAIN:PLAYER:1
STAIN:PLAYER:1 |= STAIN:5

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
A = 2
B = NO:PLAYER + 899
IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:レズ経験 += A
	CFLAG:B += A
ELSEIF TALENT:オトコ && TALENT:PLAYER:オトコ
	PRINTFORML %EXPNAME:41%+{A}
	EXP:ゲイ経験 += A
	CFLAG:B += A
ENDIF

;依存度ベクトル
TFLAG:33 = 3

;依存度補正値(服従で1、隷属で2）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

RETURN 1


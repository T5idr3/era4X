;-------------------------------------------------
;乳の揉み合い
;助手によるコマンド、及びレズプレイ、レベル1
;-------------------------------------------------
@COM126
PRINTL 乳の揉み合い
STR:0 = 乳の揉み合い
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:0 += 7
LOSEBASE:1 += 70

SOURCE:3 = 200
SOURCE:5 = 150
SOURCE:13 = 50
SOURCE:15 = 30
SOURCE:17 = 100
SOURCE:18 = 200


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:16 += 100

;調教者が［幼児退行］
IF TALENT:PLAYER:幼児／幼児退行
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:5 , 1.20
ENDIF

;調教者が［幼稚］
IF TALENT:PLAYER:幼稚
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:5 , 1.20
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＢ⇔調教者の指の汚れが移動
	STAIN:5 |= STAIN:PLAYER:1
	STAIN:PLAYER:1 |= STAIN:5

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
A = 7
B = NO:PLAYER + 899
IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:レズ経験 += A
	CFLAG:B += A
ELSEIF TALENT:オトコ && TALENT:PLAYER:オトコ
	PRINTFORML %EXPNAME:41%+{A}
	EXP:ゲイ経験 += A
	CFLAG:B += A
ENDIF

;依存度ベクトル
P = NO:PLAYER
IF RELATION:P > 100
	TFLAG:33 = 2
ELSEIF RELATION:P < 100
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 1
ENDIF

;依存度補正値(相性100↑かつ淫乱で1、娼婦で2）
P = NO:PLAYER
IF TALENT:淫乱 && RELATION:P > 100
	TFLAG:30 += 1
ELSEIF TALENT:娼婦 && RELATION:P > 100
	TFLAG:30 += 2
ENDIF


RETURN 1


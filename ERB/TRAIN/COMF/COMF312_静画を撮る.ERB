LOCAL:2 = 0;ターゲットがプレイヤーにもたらす射精感
LOCAL:4 = 0;スケベレベル。0で通常、1でスケベ、2でドスケベ
LOCAL:7 = 0;ホモチェック。端的に言うとターゲットに男が混じってるか

;エロ撮影検知
FOR LOCAL:0, 0, MTAR_NUM
	LOCAL:1 = MTAR:(LOCAL:0)
	CALL COM_UNIQUE_SUKEBECHECK(LOCAL:1)
	LOCAL:4 = RESULT
NEXT


;まずターゲットから
FOR LOCAL:0, 0, MTAR_NUM
	LOCAL:1 = MTAR:(LOCAL:0)
	
	;エロ撮影でなければスキンシップより弱い程度の効果になる
	;この部分はエロであろうとなかろうと共通で入る
	;親密に応じた歓楽のソース追加
	SOURCE:(LOCAL:1):歓楽 = -30
	CALL ADD_SOURCE_KANRAKU(LOCAL:1, 120)
	SOURCE:(LOCAL:1):歓楽 = MAX(SOURCE:(LOCAL:1):歓楽, 0)

	;親密に応じた愛情のソース追加
	CALL ADD_SOURCE_AIZYOU(LOCAL:1, 70)
	
	;以下、エロだった場合の加算事項
	IF LOCAL:4 >= 1
		;ターゲットの欲望、露出に応じて歓楽が入る
		;性格によって反感補正もあるが、欲望と露出が育てばいずれ問題なくなる。個人差はデカイが
		LOCAL:5 = 50
		LOCAL:6 = 80
		SELECTCASE ABL:(LOCAL:1):露出
			CASE 0
				TIMES LOCAL:5, 0.50
				TIMES LOCAL:6, 1.00
			CASE 1
				TIMES LOCAL:5, 1.00
				TIMES LOCAL:6, 0.90
			CASE 2
				TIMES LOCAL:5, 1.50
				TIMES LOCAL:6, 0.80
			CASE 3
				TIMES LOCAL:5, 2.00
				TIMES LOCAL:6, 0.70
			CASE 4
				TIMES LOCAL:5, 2.50
				TIMES LOCAL:6, 0.60
			CASEELSE
				LOCAL:5 = LOCAL:5 * ((ABL:(LOCAL:1):露出 - 5) * 10 + 250) / 100
				LOCAL:6 = LOCAL:6 * (50 - ((ABL:(LOCAL:1):露出 - 5) * 2)) / 100
		ENDSELECT
		
		SELECTCASE ABL:(LOCAL:1):欲望
			CASE 0
				TIMES LOCAL:5, 0.75
				TIMES LOCAL:6, 1.00
			CASE 1
				TIMES LOCAL:5, 1.00
				TIMES LOCAL:6, 0.95
			CASE 2
				TIMES LOCAL:5, 1.25
				TIMES LOCAL:6, 0.90
			CASE 3
				TIMES LOCAL:5, 1.50
				TIMES LOCAL:6, 0.85
			CASE 4
				TIMES LOCAL:5, 1.75
				TIMES LOCAL:6, 0.80
			CASEELSE
				LOCAL:5 = LOCAL:5 * ((ABL:(LOCAL:1):欲望 - 5) * 10 + 175) / 100
				LOCAL:6 = LOCAL:6 * (75 - (ABL:(LOCAL:1):欲望 - 5)) / 100
		ENDSELECT
		
		IF TALENT:(LOCAL:1):自制心
			TIMES LOCAL:5, 0.80
			TIMES LOCAL:6, 1.30
		ENDIF
		IF TALENT:(LOCAL:1):好奇心
			TIMES LOCAL:5, 1.20
			TIMES LOCAL:6, 0.90
		ENDIF
		IF TALENT:(LOCAL:1):保守的
			TIMES LOCAL:5, 0.70
			TIMES LOCAL:6, 1.50
		ENDIF
		IF TALENT:(LOCAL:1):目立ちたがり
			TIMES LOCAL:5, 2.00
			TIMES LOCAL:6, 0.50
		ENDIF
		IF TALENT:(LOCAL:1):貞操観念
			TIMES LOCAL:5, 0.50
			TIMES LOCAL:6, 2.00
		ENDIF
		IF TALENT:(LOCAL:1):貞操無頓着
			TIMES LOCAL:5, 1.50
			TIMES LOCAL:6, 0.70
		ENDIF
		IF TALENT:(LOCAL:1):抑圧
			TIMES LOCAL:5, 0.60
			TIMES LOCAL:6, 1.80
		ENDIF
		IF TALENT:(LOCAL:1):解放
			TIMES LOCAL:5, 1.10
			TIMES LOCAL:6, 0.90
		ENDIF
		IF TALENT:(LOCAL:1):孤高
			TIMES LOCAL:5, 0.25
			TIMES LOCAL:6, 4.00
		ENDIF
		IF TALENT:(LOCAL:1):恥じらい
			TIMES LOCAL:5, 0.80
			TIMES LOCAL:6, 1.30
		ENDIF
		IF TALENT:(LOCAL:1):恥薄い
			TIMES LOCAL:5, 1.20
			TIMES LOCAL:6, 0.80
		ENDIF
		IF TALENT:(LOCAL:1):快感に素直
			TIMES LOCAL:5, 1.30
			TIMES LOCAL:6, 0.70
		ENDIF
		IF TALENT:(LOCAL:1):快感の否定
			TIMES LOCAL:5, 0.25
			TIMES LOCAL:6, 4.00
		ENDIF
		IF GETBIT(TALENT:(LOCAL:1):淫乱系, 素質_淫乱_淫乱)
			TIMES LOCAL:5, 2.00
			TIMES LOCAL:6, 0.50
		ENDIF
		IF GETBIT(TALENT:(LOCAL:1):淫乱系, 素質_淫乱_露出狂)
			TIMES LOCAL:5, 4.00
			TIMES LOCAL:6, 0.25
		ENDIF
	
		;プレイヤーの射精感
		;ターゲットの露出、欲望によってプレイヤーのもよおす射精感が高くなる
		;ほぼイコールでズリネタ確保なので、撮影中に射精しても仕方ないということにして、基準値は見せ系より大きい
		LOCAL:3 = 60 + 60 * LOCAL:4
		SELECTCASE ABL:(LOCAL:1):露出
			CASE 0
				TIMES LOCAL:3, 0.50
			CASE 1
				TIMES LOCAL:3, 1.00
			CASE 2
				TIMES LOCAL:3, 1.60
			CASE 3
				TIMES LOCAL:3, 2.20
			CASE 4
				TIMES LOCAL:3, 2.80
			CASEELSE
				LOCAL:3 = LOCAL:3 * ((ABL:(LOCAL:1):露出 - 5) * 10 + 290) / 100
		ENDSELECT
		
		SELECTCASE ABL:(LOCAL:1):欲望
			CASE 0
				TIMES LOCAL:3, 0.70
			CASE 1
				TIMES LOCAL:3, 1.00
			CASE 2
				TIMES LOCAL:3, 1.30
			CASE 3
				TIMES LOCAL:3, 1.60
			CASE 4
				TIMES LOCAL:3, 2.00
			CASEELSE
				LOCAL:3 = LOCAL:3 * ((ABL:(LOCAL:1):欲望 - 5) * 10 + 210) / 100
		ENDSELECT
	
		;ここまで出た数値に場所で補正をかける
		;遊泳場。水着姿
		IF GET_PLACENAME(TFLAG:54) == "遊泳場"
			TIMES LOCAL:3, 1.50
			TIMES LOCAL:5, 1.00;ターゲットが露出好きなら歓楽に補正あってもいいがここでさらにその辺加味した倍率かけると大きくなりすぎる懸念から
			TIMES LOCAL:6, 1.50;反感は初期値で頭打ちなのでそのまま倍率かける
		ENDIF
		
		;温泉。すっぽんぽん
		IF GET_PLACENAME(TFLAG:54) == "温泉"
			TIMES LOCAL:3, 2.00
			TIMES LOCAL:5, 1.00;同上
			TIMES LOCAL:6, 2.00
		ENDIF
	
		SOURCE:(LOCAL:1):露出 = 300 * LOCAL:4
		SOURCE:(LOCAL:1):逸脱 = 60
		SOURCE:(LOCAL:1):歓楽 = LOCAL:5
		SOURCE:(LOCAL:1):反感 = LOCAL:6
		
		;主導権に応じたソースの追加
		CALL ADD_SOURCE_INITIATIVE_N(LOCAL:1, 50, 50)
		
		;プレイヤーの数だけ露出経験加算
		EXP:(LOCAL:1):露出経験値 += MPLY_NUM
		
		LOCAL:2 += LOCAL:3
		
		;ターゲットに男が混じってる検知
		SIF !IS_FEMALE(LOCAL:1)
			LOCAL:7 += 1
	ENDIF
		
	;プレイヤーの数だけ撮影経験加算
	EXP:(LOCAL:1):撮影経験 += MPLY_NUM
NEXT

;以下プレイヤー
FOR LOCAL:0, 0, MPLY_NUM

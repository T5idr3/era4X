				LOCAL:6 = LOCAL:6 * (50 - (ABL:(LOCAL:1):欲望 - 5) * 2) / 100
		ENDSELECT
		
		IF TALENT:(LOCAL:1):自制心
			TIMES LOCAL:5, 0.80
			TIMES LOCAL:6, 1.30
		ENDIF
		IF TALENT:(LOCAL:1):好奇心
			TIMES LOCAL:5, 1.20
			TIMES LOCAL:6, 0.90
		ENDIF
		IF TALENT:(LOCAL:1):保守的
			TIMES LOCAL:5, 0.70
			TIMES LOCAL:6, 1.50
		ENDIF
		IF TALENT:(LOCAL:1):貞操観念
			TIMES LOCAL:5, 0.50
			TIMES LOCAL:6, 2.00
		ENDIF
		IF TALENT:(LOCAL:1):貞操無頓着
			TIMES LOCAL:5, 1.50
			TIMES LOCAL:6, 0.70
		ENDIF
		IF TALENT:(LOCAL:1):抑圧
			TIMES LOCAL:5, 0.60
			TIMES LOCAL:6, 1.80
		ENDIF
		IF TALENT:(LOCAL:1):解放
			TIMES LOCAL:5, 1.10
			TIMES LOCAL:6, 0.90
		ENDIF
		IF TALENT:(LOCAL:1):孤高
			TIMES LOCAL:5, 0.25
			TIMES LOCAL:6, 4.00
		ENDIF
		IF TALENT:(LOCAL:1):恥じらい
			TIMES LOCAL:5, 0.80
			TIMES LOCAL:6, 1.30
		ENDIF
		IF TALENT:(LOCAL:1):恥薄い
			TIMES LOCAL:5, 1.20
			TIMES LOCAL:6, 0.80
		ENDIF
		IF TALENT:(LOCAL:1):快感に素直
			TIMES LOCAL:5, 1.30
			TIMES LOCAL:6, 0.70
		ENDIF
		IF TALENT:(LOCAL:1):快感の否定
			TIMES LOCAL:5, 0.25
			TIMES LOCAL:6, 4.00
		ENDIF
		IF GETBIT(TALENT:(LOCAL:1):淫乱系, 素質_淫乱_淫乱)
			TIMES LOCAL:5, 2.00
			TIMES LOCAL:6, 0.50
		ENDIF
		
		
		;ここまで出た数値に場所で補正をかける
		;遊泳場。水着姿
		IF GET_PLACENAME(TFLAG:54) == "遊泳場"
			TIMES LOCAL:5, 1.50
			TIMES LOCAL:6, 1.50
		ENDIF
		
		;温泉。すっぽんぽん
		IF GET_PLACENAME(TFLAG:54) == "温泉"
			TIMES LOCAL:5, 2.00
			TIMES LOCAL:6, 2.00
		ENDIF
		
		;俺はホモじゃねえ！補正。ここまでが台無しになるレベルの激烈な補正
		;股見せに男が混じってる＆両刀じゃない＆女じゃない
		IF !LOCAL:7 == 0 && !TALENT:(LOCAL:1):両刀 && !IS_FEMALE(LOCAL:1)
			TIMES LOCAL:5, 0.25
			TIMES LOCAL:6, 16.00
		ENDIF
		
		;まず感応に加算
		SOURCE:(LOCAL:1):感応 = LOCAL:2
		
		;ちんこがあったら射精感
		IF HAS_PENIS(LOCAL:1)
			;男が混じってないか
			IF LOCAL:7 == 0
				LOCAL:3 = PALAM:(LOCAL:1):射精 + LOCAL:2
				PALAM:(LOCAL:1):射精 = LOCAL:3
				;射精寸前だったら最後の一押し
				IF LOCAL:3 >= 10000
					SOURCE:(LOCAL:1):快Ｐ = 1
				ENDIF
			ELSE
				;混じってても両刀ならイケます
				IF TALENT:(LOCAL:1):両刀
					IF LOCAL:3 == 0
						LOCAL:3 = PALAM:(LOCAL:1):射精 + LOCAL:2
						PALAM:(LOCAL:1):射精 = LOCAL:3
						;射精寸前だったら最後の一押し
						IF LOCAL:3 >= 10000
							SOURCE:(LOCAL:1):快Ｐ = 1
						ENDIF
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	
		SOURCE:(LOCAL:1):歓楽 = LOCAL:5
		SOURCE:(LOCAL:1):反感 = LOCAL:6
	ENDIF
	
	;主導権に応じたソースの追加
	CALL ADD_SOURCE_INITIATIVE_N(LOCAL:1, 50, 50)

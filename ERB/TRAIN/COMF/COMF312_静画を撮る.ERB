	RETURN 0
;プレイヤーが1人以上
SIF MPLY_NUM <= 0
	RETURN 0
;ターゲットが1人以上3人以下
SIF MTAR_NUM <= 0 || MTAR_NUM >= 4
	RETURN 0
;主人公以外が実行する場合、好感度が500以上必要
SIF MPLY:0 != MASTER && CFLAG:(MPLY:0):2 < 500
	RETURN 0
;怪我なら不可
SIF CFLAG:(MTAR:0):行動不能状態 == 3
	RETURN 0
RETURN 1


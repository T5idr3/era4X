				IF NOWEX:CHECK_AUTO_MEQUIP_PLAYER_NUM:射精 >= 1 && RAND:100 >= PROBABILITY_NOT_PULL_OUT - MEQUIP_TURN:CHECK_SEARCH_EQUIP * PROBABILITY_PULL_OUT_MULTI
					WAIT

STATUS:DAMAGE = 40
STATUS:FIRE_RATE = 6
STATUS:HIT_RATE = 70
STATUS:FIRE_RANGE = 4
STATUS:SIZE = 4
STATUS:AIR_SHIP_HP = 250
STATUS:ANTI_AIR = 100
STATUS:AIR_SHIP_COUNT = 60

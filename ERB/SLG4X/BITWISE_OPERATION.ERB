;与えられた勢力にとっての敵勢力を・各ビットに入れて返す変数
@ENEMY_COUNTRY_BITWISE_OPERATION(MAIN_COUNTRY)
#DIM MAIN_COUNTRY
#DIM CHECK_COUNTRY
#DIM ENEMY_COUNTRY

ENEMY_COUNTRY = 0

FOR CHECK_COUNTRY, 0, MAX_COUNTRY
	IF REL_LIKE_COUNTRY:MAIN_COUNTRY:CHECK_COUNTRY <= -1000
		SETBIT ENEMY_COUNTRY, CHECK_COUNTRY
	ENDIF
NEXT

RETURN ENEMY_COUNTRY
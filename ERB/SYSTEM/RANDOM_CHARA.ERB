; ランダムキャラ作成
@RANDOM_CHARA(TYPE, PRICE)
#DIM TYPE
#DIM NEW_CHARA_ID

RESULT = 0
ADDVOIDCHARA
NEW_CHARA_ID = CHARANUM - 1
WHILE RESULT != 1
	DRAWLINE
	T = NEW_CHARA_ID
	CALL RANDMIZE_STATUS(NEW_CHARA_ID, TYPE)
	CALL SHOW_INFO(NEW_CHARA_ID)
	DRAWLINE
	PRINTBUTTON "[1]決定", 1
	PRINTBUTTON "[2]次へ", 2
	PRINTBUTTON "[3]戻る", 3
	INPUT
	IF RESULT == 1
		IF MONEY < PRICE
			PRINTFORMW 所持金が足りません
			RETURN
		ENDIF
		MONEY -= PRICE
		CALL INPUT_NAME(NEW_CHARA_ID)
		CALL INIT_ROUTE(NEW_CHARA_ID)
		CHARACTER_POSITION:NEW_CHARA_ID = CHARACTER_POSITION:0
		RESULT = 1
	ELSEIF RESULT == 3
		DELCHARA (NEW_CHARA_ID)
		RETURN
	ENDIF
WEND


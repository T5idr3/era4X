@INPUT_NAME(ID)
#DIM ID
#DIMS NEW_NAME
#DIMS NEW_CALLNAME
$INPUT_LOOP
PRINTFORML %CALLNAME:ID%の新しい名前を入力してください
INPUTS
NEW_NAME = %RESULTS%
SIF STRLENS(RESULTS) == 0
	GOTO INPUT_LOOP
$INPUT_CALLNAME
PRINTFORML %CALLNAME:ID%の新しい呼び名を入力してください
INPUTS
SIF STRLENS(RESULTS) == 0
	GOTO INPUT_CALLNAME
NEW_CALLNAME = %RESULTS%

$INPUT_LOOP2
PRINTFORML 以下でよろしいでしょうか
PRINTFORML 名前  : %NEW_NAME%
PRINTFORML 呼び名: %NEW_CALLNAME%

PRINTBUTTON "[1]決定", 1
PRINTBUTTON "[2]やり直す", 2
INPUT
IF RESULT == 1
	NAME:ID = %NEW_NAME%
	CALLNAME:ID = %NEW_CALLNAME%
ELSEIF RESULT == 2
	GOTO INPUT_LOOP
ELSE
	GOTO INPUT_LOOP2
ENDIF
RETURN 1


;乱交ライブで歌う奴隷を選択
PRINTL 乱交ライブを行う奴隷を選択してください
REPEAT CHARANUM
	SIF COUNT == MASTER
		CONTINUE
	SIF CFLAG:COUNT:1 < 2
		CONTINUE
	SIF CFLAG:COUNT:12 > 0
		CONTINUE
	SIF (TALENT:COUNT:淫乱 || TALENT:COUNT:娼婦) && BASE:COUNT:0 > 800
		PRINTFORML [{COUNT}] - %CALLNAME:COUNT%
REND
PRINTL [100] - 戻る
$INPUT_LOOP_2
INPUT
IF RESULT == 100
	RETURN 0
ELSEIF RESULT < 0 || RESULT >= CHARANUM
	GOTO INPUT_LOOP_2
ELSEIF RESULT == MASTER
	GOTO INPUT_LOOP_2
ELSEIF CFLAG:RESULT:1 < 2
	GOTO INPUT_LOOP_2
ELSEIF CFLAG:RESULT:12 > 0
	GOTO INPUT_LOOP_2
ELSEIF (TALENT:RESULT:淫乱 == 0 && TALENT:RESULT:娼婦 == 0) || BASE:RESULT:0 <= 800
	GOTO INPUT_LOOP_2
ELSE
	A = RESULT
ENDIF

$INPUT_LOOP_3
IF CFLAG:A:108
	PRINTL <避妊結界有効>
	J = 1
ELSE
	PRINTL 避妊の有無と方法を選択してください
	PRINTL [0]   - 避妊しない
	SIF MONEY > 2000
		PRINTFORML [1]   - 子宮内避妊具($-6000）
	PRINTL [100] -   戻る
	INPUT
	IF RESULT == 0
		J = 0
	ELSEIF RESULT == 1 && MONEY > 6000
		J = 1
	ELSEIF RESULT == 100
		RETURN 0
	ELSE
		GOTO INPUT_LOOP_3
	ENDIF
ENDIF

;ライブ撮影オプション
$INPUT_LOOP_4
I = -1
IF (ITEM:18 > 0 && ITEM:43 > 0) || NOITEM != 0
	PRINTL 乱交ライブを撮影する者を選択してください
	PRINTFORML [0] - %CALLNAME:MASTER%
	REPEAT CHARANUM
		SIF COUNT == MASTER
			CONTINUE
		SIF CFLAG:COUNT:12 > 0
			CONTINUE
		SIF CFLAG:COUNT:1 > 2
			PRINTFORML [{COUNT}] - %CALLNAME:COUNT%
	REND
	PRINTL [100] - 撮影しない
	INPUT
	IF RESULT == 100
		PRINTL 
	ELSEIF RESULT < 0 || RESULT >= CHARANUM
		GOTO INPUT_LOOP_4
	ELSEIF CFLAG:RESULT:1 < 2 && RESULT != MASTER
		GOTO INPUT_LOOP_4
	ELSEIF CFLAG:RESULT:12 > 0
		GOTO INPUT_LOOP_4
	ELSEIF RESULT == MASTER
		V += 150+ABL:MASTER:13*10
		I = MASTER
		PRINTFORML 撮影者は%CALLNAME:MASTER%です
		PRINTFORML ライブ撮影 +{V}％
		PRINTL 
	ELSE
		V += 150+ABL:RESULT:13*10
		I = RESULT
		PRINTFORML 撮影者は%CALLNAME:RESULT%です
		PRINTFORML ライブ撮影 +{V}％
		PRINTL 

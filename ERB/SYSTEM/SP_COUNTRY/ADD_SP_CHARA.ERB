            CASE IS < 40
                TALENT:対象:性別 = 1
            CASE IS < 80
                TALENT:対象:性別 = 2
            CASE IS < 90
                TALENT:対象:性別 = 4
            CASE IS < 100
                TALENT:対象:性別 = 5
        ENDSELECT
    ELSEIF GROUPMATCH(勢力定数, 特殊勢力_外来人, 特殊勢力_自警団, 特殊勢力_触手, 特殊勢力_狂信者)
        SELECTCASE RAND:100
            CASE IS < 60
                TALENT:対象:性別 = 0
            CASE IS < 90
                TALENT:対象:性別 = 1
            CASE IS < 100
                TALENT:対象:性別 = 2
        ENDSELECT
    ENDIF

@GET_MEMBER_NUM(MEMBER)
#FUNCTION
#DIM REF MEMBER
#DIM CNT

CNT = 0
FOR LOCAL, 0, MAX_MEMBER
  SIF MEMBER:LOCAL != 0
    CNT ++
NEXT
RETURNF CNT
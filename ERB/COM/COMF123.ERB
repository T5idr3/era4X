;-------------------------------------------------
;足コキ
;助手によるコマンド、及びレズプレイ、レベル1
;-------------------------------------------------
@COM123
PRINTL 足コキ
STR:0 = 足コキ
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:0 += 30
LOSEBASE:1 += 150

SOURCE:0 = 250
SOURCE:6 = 200
SOURCE:13 = 300
SOURCE:14 = 300
SOURCE:17 = 350
SOURCE:18 = 350


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:16 += 400

;苦痛を伴わないマゾっ気及びマゾの処理は先にしておく
;マゾっ気を見る
IF ABL:9 == 0
	TIMES SOURCE:6 , 0.80
	TIMES SOURCE:14 , 0.80
	SOURCE:10 += 0
	SOURCE:11 += 0
ELSEIF ABL:9 == 1
	TIMES SOURCE:6 , 1.00
	TIMES SOURCE:14 , 1.00
	SOURCE:10 += 20
	SOURCE:11 += 30
ELSEIF ABL:9 == 2
	TIMES SOURCE:6 , 1.20
	TIMES SOURCE:14 , 1.20
	SOURCE:10 += 40
	SOURCE:11 += 70
ELSEIF ABL:9 == 3
	TIMES SOURCE:6 , 1.40
	TIMES SOURCE:14 , 1.40
	SOURCE:10 += 70
	SOURCE:11 += 120
ELSEIF ABL:9 == 4
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:14 , 1.50
	SOURCE:10 += 110
	SOURCE:11 += 180
ELSEIF ABL:9 == 5
	TIMES SOURCE:6 , 1.70
	TIMES SOURCE:14 , 1.70
	SOURCE:10 += 150
	SOURCE:11 += 250
ELSEIF ABL:9 == 6
	TIMES SOURCE:6 , 1.90
	TIMES SOURCE:14 , 1.90
	SOURCE:10 += 200
	SOURCE:11 += 300
ELSEIF ABL:9 == 7
	TIMES SOURCE:6 , 2.10
	TIMES SOURCE:14 , 2.10
	SOURCE:10 += 250
	SOURCE:11 += 350
ELSEIF ABL:9 == 8
	TIMES SOURCE:6 , 2.30
	TIMES SOURCE:14 , 2.30
	SOURCE:10 += 300
	SOURCE:11 += 400
ELSEIF ABL:9 == 9
	TIMES SOURCE:6 , 2.50
	TIMES SOURCE:14 , 2.50
	SOURCE:10 += 350
	SOURCE:11 += 450
ELSEIF ABL:9 >= 10
	TIMES SOURCE:6 , 3.00
	TIMES SOURCE:14 , 3.00
	SOURCE:10 += 500
	SOURCE:11 += 500
ENDIF

;マゾを見る
IF TALENT:マゾ == 1
	TIMES SOURCE:6 , 2.50
	TIMES SOURCE:14 , 2.50
	TIMES SOURCE:10 , 1.20
	TIMES SOURCE:11 , 1.20
ENDIF

;倒錯的
SIF TALENT:倒錯的
	TIMES SOURCE:6 , 1.80

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
A = 4
B = NO:PLAYER + 899
IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:レズ経験 += A
	CFLAG:B += A
ELSEIF TALENT:オトコ && TALENT:PLAYER:オトコ
	PRINTFORML %EXPNAME:41%+{A}
	EXP:ゲイ経験 += A
	CFLAG:B += A
ENDIF

;依存度ベクトル
P = NO:PLAYER
IF RELATION:P > 100
	TFLAG:33 = 2
ELSEIF RELATION:P < 100
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 1
ENDIF

;依存度補正値(相性100↑かつ淫乱で1、娼婦で2）
P = NO:PLAYER
IF TALENT:淫乱 && RELATION:P > 100
	TFLAG:30 += 1
ELSEIF TALENT:娼婦 && RELATION:P > 100
	TFLAG:30 += 2
ENDIF



RETURN 1


;-------------------------------------------------
;アナルフィスト
;系コマンド、レベル
;-------------------------------------------------
@COM142
PRINTL アナルフィスト
STR:0 = アナルフィスト
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:0 += 300
LOSEBASE:1 += 150

SOURCE:2 = 200
SOURCE:7 = 1500
SOURCE:13 = 100
SOURCE:14 = 1200
SOURCE:17 = 1200
SOURCE:18 = 1200


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;フィストなどのAへの大きな苦痛は先に処理しておく
;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:7 , 3.00
	TIMES SOURCE:18 , 4.00
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:7 , 2.50
	TIMES SOURCE:18 , 3.50
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:7 , 2.40
	TIMES SOURCE:18 , 3.00
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:18 , 2.50
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:7 , 1.80
	TIMES SOURCE:18 , 2.00
ELSEIF EXP:Ａ経験 >= EXPLV:5
	TIMES SOURCE:7 , 1.60
	TIMES SOURCE:18 , 1.50
ENDIF

;PALAM:潤滑をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:7 , 3.00
	TIMES SOURCE:18 , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:7 , 2.50
	TIMES SOURCE:18 , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:18 , 1.20
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:7 , 1.50
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:7 , 1.00
ENDIF


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;Ａ経験
EXP:Ａ経験 += 25
PRINTL Ａ経験＋２５

;Ａ拡張経験
IF EXP:Ｖ拡張経験 == 0 && EXP:Ａ拡張経験 == 0 
	EXP:異常経験 += 1
	PRINTL 異常経験＋１
ENDIF

EXP:Ａ拡張経験 += 5
PRINTL Ａ拡張経験＋5

;レズ経験
A = 1
B = NO:PLAYER + 899
IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:レズ経験 += A
	CFLAG:B += A
ELSEIF TALENT:オトコ && TALENT:PLAYER:オトコ
	PRINTFORML %EXPNAME:41%+{A}
	EXP:ゲイ経験 += A
	CFLAG:B += A
ENDIF

;依存度ベクトル
TFLAG:33 = 5

;依存度補正値(服従で1、隷属で2）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1


;-------------------------------------------------
;挿入Ｇスポ責め
;セックス系派生コマンド
;-------------------------------------------------
@COM506
PRINTL 挿入Ｇスポ責め
STR:0 = 挿入Ｇスポ責め
SELECTCOM = 506
CALL KOJO_MESSAGE_COM

;V経験を伴うコマンドのフラグ(処女膜再生対応)
TFLAG:19 = 1
SIF (TALENT:恋慕 || TALENT:親愛) && ASSIPLAY == 0 && EXP:Ｖ経験 == 0
	TFLAG:20 = 1

;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
B = 1800

;ABL:調教者の技巧をみる
IF ABL:PLAYER:2 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:2 == 1
	TIMES B , 1.20
ELSEIF ABL:PLAYER:2 == 2
	TIMES B , 1.40
ELSEIF ABL:PLAYER:2 == 3
	TIMES B , 1.60
ELSEIF ABL:PLAYER:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:欲望をみる
IF ABL:1 == 0
	TIMES B , 1.00
ELSEIF ABL:1 == 1
	TIMES B , 1.10
ELSEIF ABL:1 == 2
	TIMES B , 1.20
ELSEIF ABL:1 == 3
	TIMES B , 1.30
ELSEIF ABL:1 == 4
	TIMES B , 1.40
ELSE
	TIMES B , 1.50
ENDIF

;PALAM:潤滑をみる
IF PALAM:4 < PALAMLV:1
	TIMES B , 0.60
ELSEIF PALAM:4 < PALAMLV:2
	TIMES B , 0.80
ELSEIF PALAM:4 < PALAMLV:3
	TIMES B , 1.00
ELSEIF PALAM:4 < PALAMLV:4
	TIMES B , 1.20
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES B , 1.40
ENDIF

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;EXP:Ｖ経験をみる
;処女だと増える
SIF EXP:Ｖ経験 < EXPLV:1
	TIMES B , 1.50

;再装填処理中は射精ゲージは20分の1に
SIF CFLAG:PLAYER:24 > 0
	B /= 20


SIF TALENT:PLAYER:半人半妖 || TALENT:PLAYER:ふたなり || TALENT:PLAYER:オトコ
	BASE:PLAYER:2 += B


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:0 += 60
LOSEBASE:1 += 120

SOURCE:1 = 500
SOURCE:4 = 200
SOURCE:7 = 500
SOURCE:9 = 300
SOURCE:13 = 100
SOURCE:18 = 500


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:16 += 500

;貞操観念持ちで恋慕、親愛持ち以外には常に反発のソースを追加する（セックス系のみ）
SIF TALENT:貞操観念 && (TALENT:恋慕 == 0 && TALENT:親愛 == 0)
	SOURCE:18 += 1000

;処女だった場合は、苦痛のソースと反発のソースを追加する
IF TALENT:処女 == 1
	SOURCE:7 += 2000
	SOURCE:18 += 3000
ENDIF

;再生処女だった場合は、苦痛のソースと反発のソースを追加する
IF TALENT:再生処女 == 1
	SOURCE:7 += 500
	SOURCE:18 += 1000
ENDIF

;Vへの苦痛は先に処理しておく
;EXP:Ｖ経験をみる（処女の反感は別途処理済みなので省略）
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:7 , 2.50
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:7 , 0.80
	TIMES SOURCE:18 , 1.20
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:7 , 0.40
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:7 , 0.10
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:7 , 0.00
ELSEIF EXP:Ｖ経験 >= EXPLV:5
	TIMES SOURCE:7 , 0.00
ENDIF

;PALAM:潤滑をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:7 , 1.20
	TIMES SOURCE:18 , 1.50
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:7 , 0.70
	TIMES SOURCE:18 , 1.20
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:7 , 0.40
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:7 , 0.10
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:7 , 0.10
ENDIF

;中毒による中毒充足のソースは先に処理しておく
;セックス中毒を見る
IF ABL:18 == 1
	TIMES SOURCE:9 , 0.00
ELSEIF ABL:18 == 2
	TIMES SOURCE:9 , 0.30
ELSEIF ABL:18 == 3
	TIMES SOURCE:9 , 0.60
ELSEIF ABL:18 == 4
	TIMES SOURCE:9 , 0.90
ELSEIF ABL:18 == 5
	TIMES SOURCE:9 , 1.20
ELSEIF ABL:18 == 6
	TIMES SOURCE:9 , 1.50
ELSEIF ABL:18 == 7
	TIMES SOURCE:9 , 1.80
ELSEIF ABL:18 == 8
	TIMES SOURCE:9 , 2.30
ELSEIF ABL:18 == 9
	TIMES SOURCE:9 , 3.00
ELSEIF ABL:18 >= 10
	TIMES SOURCE:9 , 5.00
ENDIF


;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK

;射精時の処理
IF E >= 1
	;射精先タイプの設定
	TFLAG:703 = 1
	;射精先選択の可・不可
	TFLAG:704 = 1
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
;膣内に破瓜の血の汚れを付加
SIF TALENT:処女 == 1 || TALENT:再生処女 == 1
	STAIN:6 |= 64
;奴隷の膣内⇔調教者のＰの汚れが移動
STAIN:6 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:6

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ｖ経験 += 1
PRINTL Ｖ経験＋１

A = 4
B = NO:PLAYER + 899
IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:レズ経験 += A
	CFLAG:B += A
ELSEIF TALENT:オトコ && TALENT:PLAYER:オトコ
	PRINTFORML %EXPNAME:41%+{A}
	EXP:ゲイ経験 += A
	CFLAG:B += A
ENDIF

;愛情経験
IF TALENT:恋慕 == 1 && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +1
	EXP:愛情経験 += 1
ELSEIF TALENT:親愛 == 1 && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +2
	EXP:愛情経験 += 2
ENDIF

;依存度ベクトル
TFLAG:33 = 1

;依存度補正値(恋慕で1、親愛で2）
IF TALENT:恋慕 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:親愛 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1


;-------------------------------------------------
;胸自慰
;愛撫系コマンド、レベル3
;-------------------------------------------------
@COM10
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;前コマンドが騎乗位の場合は騎乗乳房自慰にジャンプ
SIF PREVCOM == 66 && TFLAG:60
	JUMP COM505
;前コマンドが騎乗位アナルの場合は騎乗乳房自慰にジャンプ
SIF PREVCOM == 67 && TFLAG:60
	JUMP COM505

SIF TEQUIP:28
	PRINT 公開
PRINTL 胸自慰

;実行者が奴隷であるフラグ
TFLAG:28 = 1


;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;共通処理
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0
	
STR:0 = 胸自慰
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:0 += 5
LOSEBASE:1 += 50

SOURCE:3 = 50
SOURCE:5 = 40
SOURCE:8 = 100
SOURCE:9 = 100
SOURCE:13 = 200
SOURCE:14 = 400
SOURCE:17 = 150
SOURCE:18 = 150
IF TEQUIP:28
	SOURCE:10 = 50
	SOURCE:17 += 100
ENDIF


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:16 += 150

;中毒による中毒充足のソースは先に処理しておく
;自慰中毒を見る
SIF ABL:15 == 1
	TIMES SOURCE:9 , 0.00
SIF ABL:15 == 2
	TIMES SOURCE:9 , 0.30
SIF ABL:15 == 3
	TIMES SOURCE:9 , 0.60
SIF ABL:15 == 4
	TIMES SOURCE:9 , 0.90
SIF ABL:15 == 5
	TIMES SOURCE:9 , 1.20
SIF ABL:15 == 6
	TIMES SOURCE:9 , 1.50
SIF ABL:15 == 7
	TIMES SOURCE:9 , 1.80
SIF ABL:15 == 8
	TIMES SOURCE:9 , 2.30
SIF ABL:15 == 9
	TIMES SOURCE:9 , 3.00
SIF ABL:15 >= 10
	TIMES SOURCE:9 , 5.00


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の指⇔奴隷のＢの汚れが移動
STAIN:1 |= STAIN:5
STAIN:5 |= STAIN:1

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;ビデオ撮影時は経験にプラス
IF TEQUIP:28
	EXP:自慰経験 += 2
	PRINTL 自慰経験＋２
	EXP:調教自慰経験 += 2
	PRINTL 調教自慰経験＋２
ELSE
	EXP:自慰経験 += 1
	PRINTL 自慰経験＋１
	EXP:調教自慰経験 += 1
	PRINTL 調教自慰経験＋１
ENDIF
IF TEQUIP:28 && CFLAG:3 == 0
	EXP:異常経験 += 1
	PRINTL 異常経験＋１
	CFLAG:3 = 1
ENDIF

A = 3
B = NO:PLAYER + 899
IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:レズ経験 += A
	CFLAG:B += A
ELSEIF TALENT:オトコ && TALENT:PLAYER:オトコ
	PRINTFORML %EXPNAME:41%+{A}
	EXP:ゲイ経験 += A
	CFLAG:B += A
ENDIF

;依存度ベクトル
TFLAG:33 = 3

;依存度補正値(服従で1、隷属で2）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1
;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_10
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	SIF S
		PRINT  + 
	A += ABL:1 * 3
	PRINTS ABLNAME:1
	PRINTV 'LV,ABL:1,'(,ABL:1 * 3,')
	S = 1
ENDIF
;ABL:露出癖
IF ABL:8
	SIF S
		PRINT  + 
	A += ABL:8 * 4
	PRINTS ABLNAME:8
	PRINTV 'LV,ABL:8,'(,ABL:8 * 4,')
	S = 1
ENDIF
;ABL:自慰中毒
IF ABL:15
	SIF S
		PRINT  + 
	A += ABL:15 * 3
	PRINTS ABLNAME:15
	PRINTV 'LV,ABL:15,'(,ABL:15 * 3,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 3
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 3,')
	S = 1
ENDIF


;PALAM:欲情
IF PALAM:6 < PALAMLV:1
	L = 0
ELSEIF PALAM:6 < PALAMLV:2
	L = 1
ELSEIF PALAM:6 < PALAMLV:3
	L = 2
ELSEIF PALAM:6 < PALAMLV:4
	L = 3
ELSEIF PALAM:6 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 3
	PRINTS PALAMNAME:6
	PRINTV 'LV,L,'(,L * 3,')
	S = 1
ENDIF

;'自制心
IF TALENT:自制心
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:20
	PRINTV '(,5,')
	S = 1
ENDIF
;恥じらい
IF TALENT:恥じらい
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:35
	PRINTV '(,5,')
	S = 1
ENDIF
;恥薄い
IF TALENT:恥薄い
	SIF S
		PRINT  + 
	A += 2
	PRINTS TALENTNAME:36
	PRINTV '(,2,')
	S = 1
ENDIF
;自慰しやすい
IF TALENT:自慰しやすい
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:60
	PRINTV '(,5,')
	S = 1
ENDIF
;快感に素直
IF TALENT:快感に素直
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:70
	PRINTV '(,5,')
	S = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感の否定 && (TALENT:淫乱 == 0 && TALENT:娼婦 == 0)
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:71
	PRINTV '(,5,')
	S = 1
ENDIF

;媚薬
IF TEQUIP:26
	SIF S
		PRINT  + 
	A += 8
	PRINTS ITEMNAME:41
	PRINTV '(,8,')
	S = 1
ENDIF

;合計を表示(33以上で実行)
PRINT  = 
PRINTV A

;難易度上昇
;公開で+10
V = 33
SIF TEQUIP:28
	V += 10

SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0
;実行できる
RETURN 1

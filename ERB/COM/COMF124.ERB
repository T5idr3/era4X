;-------------------------------------------------
;ダブルフェラ
;助手によるコマンド、及びレズプレイ、レベル2
;-------------------------------------------------
@COM124
PRINTL ダブルフェラ
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;共通処理
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0
	
;-------------------------------------------------
;実行決定
;-------------------------------------------------
STR:0 = ダブルフェラ
CALL KOJO_MESSAGE_COM

;実行者が奴隷であるフラグ
TFLAG:28 = 1

;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
B = 1500

;ABL:調教者の技巧をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSEIF ABL:2 == 3
	TIMES B , 1.60
ELSEIF ABL:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:奉仕精神をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;TALENT:舌使い、猫舌をみる
SIF TALENT:舌使い || TALENT:猫舌
	TIMES B , 2.00

;助手のABL:技巧をみる
IF ABL:ASSI:2 >= 2
	TIMES B , 1.00
ELSEIF ABL:ASSI:2 == 3
	TIMES B , 1.20
ELSEIF ABL:ASSI:2 == 4
	TIMES B , 1.50
ELSE
	TIMES B , 1.80
ENDIF

;助手のTALENT:舌使い、猫舌をみる
SIF TALENT:ASSI:舌使い || TALENT:猫舌
	TIMES B , 2.00

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:MASTER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:MASTER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:MASTER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:MASTER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:MASTER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF


SIF TALENT:MASTER:半人半妖 || TALENT:MASTER:ふたなり || TALENT:MASTER:オトコ
	BASE:MASTER:2 += B

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:0 += 10
LOSEBASE:1 += 100

SOURCE:4 = 100
SOURCE:5 = 400
SOURCE:6 = 200
SOURCE:14 = 600
SOURCE:17 = 200
SOURCE:18 = 200

;上のほうで計算した汚れデータ
SOURCE:15 = Y*40 + 100

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:16 += 400

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
;射精するのは主人なので専用の関数を呼ぶ
CALL SAMEN_CHECK_M

;射精時の処理
IF E >= 1
	;射精先タイプの設定
	TFLAG:703 = 4
	;射精先選択の可・不可
	TFLAG:704 = 1
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の口⇔調教者のＰの汚れが移動
STAIN:0 |= STAIN:MASTER:2
STAIN:MASTER:2 |= STAIN:0

;助手の口⇔調教者のＰの汚れが移動
STAIN:ASSI:0 |= STAIN:MASTER:2
STAIN:MASTER:2 |= STAIN:ASSI:0

;奉仕精神ＬＶ２以上、技巧ＬＶ２以上なら汚れをなめ取る（ボールギャグ使用時は除く）
IF ABL:7 >= 2 && ABL:2 >= 2 && TEQUIP:20 == 0
	STAIN:MASTER:2 = 2
	SIF E >= 1
		TFLAG:8 = 2
ENDIF

;奴隷と助手が共に女性でどちらかが精液中毒を持っているとき、奴隷の口⇔助手の口の汚れが移動
IF TALENT:オトコ == 0 && TALENT:ASSI:オトコ == 0 && (ABL:16 >= 1 || ABL:ASSI:16 >= 1)
	STAIN:0 |= STAIN:ASSI:2
	STAIN:ASSI:0 |= STAIN:0
	SIF E >= 1
		TFLAG:8 = 3
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
PRINTFORML %EXPNAME:22%＋１
EXP:フェラ経験 += 1

;マスターと奴隷のレズ
IF TALENT:オトコ == 0 && TALENT:MASTER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +7
	EXP:レズ経験 += 7
	A = NO:MASTER + 900
	CFLAG:A += 7
ENDIF

;助手と奴隷のレズ
IF TALENT:オトコ == 0 && TALENT:ASSI:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +7
	EXP:レズ経験 += 7
	A = NO:ASSI + 900
	CFLAG:A += 7
ENDIF

;奴隷が猫舌持ちの場合は、調教者に嗜虐快楽経験を追加
IF TALENT:TARGET:猫舌
	EXP:嗜虐快楽経験 += 1
	PRINTFORML %EXPNAME:32%＋１
ENDIF

;依存度ベクトル
P = NO:PLAYER
IF RELATION:P > 100
	TFLAG:33 = 2
ELSEIF RELATION:P < 100
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 1
ENDIF

;依存度補正値(相性100↑かつ淫乱で1、娼婦で2）
P = NO:PLAYER
IF TALENT:淫乱 && RELATION:P > 100
	TFLAG:30 += 1
ELSEIF TALENT:娼婦 && RELATION:P > 100
	TFLAG:30 += 2
ENDIF

TFLAG:100 = 1
RETURN 1

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_124

A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	SIF S
		PRINT  + 
	A += ABL:1 * 1
	PRINTS ABLNAME:1
	PRINTV 'LV,ABL:1,'(,ABL:1 * 1,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:7
	SIF S
		PRINT  + 
	A += ABL:7 * 4
	PRINTS ABLNAME:7
	PRINTV 'LV,ABL:7,'(,ABL:7 * 4,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:16
	SIF S
		PRINT  + 
	A += ABL:16 * 3
	PRINTS ABLNAME:16
	PRINTV 'LV,ABL:16,'(,ABL:16 * 3,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 1
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 1,')
	S = 1
ENDIF


;PALAM:欲情
IF PALAM:6 < PALAMLV:1
	L = 0
ELSEIF PALAM:6 < PALAMLV:2
	L = 1
ELSEIF PALAM:6 < PALAMLV:3
	L = 2
ELSEIF PALAM:6 < PALAMLV:4
	L = 3
ELSEIF PALAM:6 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 1
	PRINTS PALAMNAME:6
	PRINTV 'LV,L,'(,L * 1,')
	S = 1
ENDIF

;恥じらい
IF TALENT:恥じらい
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:35
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭鈍感
IF TALENT:汚臭鈍感
	SIF S
		PRINT  + 
	A += 1
	PRINTS TALENTNAME:61
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭敏感
IF TALENT:汚臭敏感
	PRINT  - 
	A -= 3
	PRINTS TALENTNAME:62
	PRINTV '(,3,')
	S = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF S
		PRINT  + 
	A += 6
	PRINTS TALENTNAME:63
	PRINTV '(,6,')
	S = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感の否定 && (TALENT:淫乱 == 0 && TALENT:娼婦 == 0)
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:71
	PRINTV '(,1,')
	S = 1
ENDIF
;男嫌い（恋慕、親愛で無効）
IF TALENT:男嫌い && TALENT:PLAYER:オトコ && ((TALENT:恋慕 == 0 && TALENT:親愛 == 0) || PLAYER != MASTER)
	PRINT  - 
	A -= 12
	PRINTS TALENTNAME:82
	PRINTV '(,12,')
	S = 1
ENDIF
;淫乱
IF TALENT:淫乱 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:4
	PRINTV '(,5,')
	S = 5
ENDIF
;娼婦
IF TALENT:娼婦 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 20
	PRINTS TALENTNAME:7
	PRINTV '(,10,')
	S = 10
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:ふたなり
	SIF S
		PRINT  + 
	A += 8
	PRINTS TALENTNAME:139
	PRINTV '(,8,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15

SIF TALENT:汚臭鈍感
	Y /= 3
SIF TALENT:汚臭敏感
	Y *= 2



;汚れあり
IF Y
	PRINT  - 
	A -= Y
	;汚臭鈍感
	IF TALENT:汚臭鈍感
		PRINT 汚れあり、
		PRINTS TALENTNAME:61
	ELSEIF TALENT:汚臭敏感
		PRINT 汚れあり、
		PRINTS TALENTNAME:62
	ELSE
		PRINT 汚れあり
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(34以上で実行)
PRINT  = 
PRINTV A

V = 34
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0

RETURN 1


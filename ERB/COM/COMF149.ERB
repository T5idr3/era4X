;-------------------------------------------------
;アナル電極
;ハードな調教コマンド、レベル4
;-------------------------------------------------
@COM149
PRINTL アナル電極
STR:0 = アナル電極
CALL KOJO_MESSAGE_COM

;着脱の処理
IF TEQUIP:23 == 0
	TEQUIP:23 = 1
ELSE
	TEQUIP:23 = 0
ENDIF

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:0 += 300
LOSEBASE:1 += 450

SOURCE:2 = 500
SOURCE:7 = 1500
SOURCE:13 = 300
SOURCE:14 = 800
SOURCE:17 = 1200
SOURCE:18 = 1200

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:16 += 1200

;フィストなどのAへの大きな苦痛は先に処理しておく
;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:7 , 3.00
	TIMES SOURCE:18 , 4.00
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:7 , 2.50
	TIMES SOURCE:18 , 3.50
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:7 , 2.40
	TIMES SOURCE:18 , 3.00
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:18 , 2.50
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:7 , 1.80
	TIMES SOURCE:18 , 2.00
ELSEIF EXP:Ａ経験 >= EXPLV:5
	TIMES SOURCE:7 , 1.60
	TIMES SOURCE:18 , 1.50
ENDIF

;PALAM:潤滑をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:7 , 3.00
	TIMES SOURCE:18 , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:7 , 2.50
	TIMES SOURCE:18 , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:18 , 1.20
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:7 , 1.50
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:7 , 1.00
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;Ａ経験
EXP:Ａ経験 += 5
PRINTL Ａ経験＋５

;異常経験が5未満のとき、異常経験追加
IF EXP:異常経験 < 5
	EXP:異常経験 += 1
	PRINTL 異常経験＋１
ENDIF

;レズ経験
A = 1
B = NO:PLAYER + 899
IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:レズ経験 += A
	CFLAG:B += A
ELSEIF TALENT:オトコ && TALENT:PLAYER:オトコ
	PRINTFORML %EXPNAME:41%+{A}
	EXP:ゲイ経験 += A
	CFLAG:B += A
ENDIF

;依存度ベクトル
TFLAG:33 = 5

;依存度補正値(服従で1、隷属で2）
IF TALENT:服従 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:隷属 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1


;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM149
;アナル電極

PRINTL ＜アナル電極挿入中＞

LOSEBASE:0 += 200
LOSEBASE:1 += 250

SOURCE:2 += 250
SOURCE:14 += 400
SOURCE:17 += 600


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:16 += 600

;フィストなどのAへの大きな苦痛は先に処理しておく
;EXP:Ａ経験をみる
A = 750
B = 600
IF EXP:Ａ経験 < EXPLV:1
	TIMES A , 3.00
	TIMES B , 4.00
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES A , 2.50
	TIMES B , 3.50
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES A , 2.40
	TIMES B , 3.00
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES A , 2.00
	TIMES B , 2.50
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES A , 1.80
	TIMES B , 2.00
ELSEIF EXP:Ａ経験 >= EXPLV:5
	TIMES A , 1.60
	TIMES B , 1.50
ENDIF

;PALAM:潤滑をみる
IF PALAM:4 < PALAMLV:1
	TIMES A , 3.00
	TIMES B , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES A , 2.50
	TIMES B , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES A , 2.00
	TIMES B , 1.20
ELSEIF PALAM:4 < PALAMLV:4
	TIMES A , 1.50
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES A , 1.00
ENDIF
SOURCE:7 += A
SOURCE:18 += B

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;Ａ経験
EXP:Ａ経験 += 5
PRINTL Ａ経験＋５


RETURN 1


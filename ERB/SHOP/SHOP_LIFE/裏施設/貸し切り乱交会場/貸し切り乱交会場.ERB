            _TEXT = %_TEXT% 妾
        ELSEIF TALENT:LOCAL:親愛
            _TEXT = %_TEXT% 親愛
        ELSEIF TALENT:LOCAL:恋慕
            _TEXT = %_TEXT% 恋慕
        ELSEIF TALENT:LOCAL:隷属
            _TEXT = %_TEXT% 隷属
        ELSEIF TALENT:LOCAL:服従
            _TEXT = %_TEXT% 服従
        ENDIF
        IF _SELECT_LIST:LOCAL
            SETCOLOR 0XFF9999
            PRINTBUTTON @"%_TEXT% 選択中", LOCAL
            RESETCOLOR
        ELSE
            PRINTBUTTON @"%_TEXT% 未選択", LOCAL
        ENDIF
        PRINTL
    ENDIF
NEXT

;パフォーマンスカードの定義
;-------------------------------------------------
;パフォーマンスカード名称
;-------------------------------------------------
@PERFORMANCE_CARDNAME_404
RESULTS:0 = 投げキッス

;-------------------------------------------------
;パフォーマンス分類
;-------------------------------------------------
@PERFORMANCE_CATEGORY_404(ARG:0)
#DIMS カード属性
VARSET カード属性
カード属性 '= DIC_SET(カード属性, "属性", "GESTURE")
カード属性 '= DIC_SET(カード属性, "演目", "CUTE")
RESULTS = %カード属性%



;-------------------------------------------------
;パフォーマンスコマンド名称
;-------------------------------------------------
@PERFORMANCE_COMNAME_404
RESULTS:0 = 投げキッス

;-------------------------------------------------
;アンロック条件
;-------------------------------------------------
@PERFORMANCE_AVAILABLE_404
RETURN 1

;-------------------------------------------------
;セット可能なキャラクター条件
;-------------------------------------------------
@PERFORMANCE_SETTABLE_BY_CHARA_404(ARG:0)
RETURN 1

;-------------------------------------------------
;演目で使用可能となる条件
;-------------------------------------------------
@PERFORMANCE_PLAYABLE_404
RETURN 1

;-------------------------------------------------
;メイン処理
;-------------------------------------------------
@PERFORMANCE_MAIN_404(ARG:0)
#DIMS カード効果
VARSET カード効果
カード効果 '= DIC_SET(カード効果, "抵抗値変更", "-5")
カード効果 '= DIC_SET(カード効果, "補正値変更", "0,10,0,2,")
RESULTS = %カード効果%


;-------------------------------------------------
;プレイ時のメッセージ
;-------------------------------------------------
@PERFORMANCE_PLAY_MESSAGE_404(ARG:0)
PRINTFORML %ANAME(ARG:0)%は観客に向かって悩ましく投げキッスした！

;-------------------------------------------------
;ステータス描写
;-------------------------------------------------
@PERFORMANCE_STATUS_DESC_404
RESULTS = 心理抵抗値 - 5\% 補正:CU + 10\% LE + 2\%

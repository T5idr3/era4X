                CASE 0 TO 6
                    CALL PERFORMANCE_CREATE_MOB("女")
                CASEELSE
                    CALL PERFORMANCE_CREATE_MOB("男")
            ENDSELECT
        NEXT
    ENDIF
ELSE
    ;見物時の処理
    ;一人は被害者枠を用意
    CALL PERFORMANCE_CREATE_MOB("女")
    FOR ITER, 0, RAND(7, 9)
        SELECTCASE RAND(0, 10)
            CASE 0 TO 7

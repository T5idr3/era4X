ELSEIF STRFINDU(名前, "通行人") != -1 || STRFINDU(名前, "会社員") != -1 || STRFINDU(名前, "ＯＬ") != -1 || STRFINDU(名前, "ファン") != -1 || STRFINDU(名前, "性奴隷") != -1 || STRFINDU(名前, "娼婦") != -1

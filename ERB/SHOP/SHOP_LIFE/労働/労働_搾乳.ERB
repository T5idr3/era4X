@WORK_MILK_TEXT(対象)
#DIM 対象
#DIM 噴乳量

PRINTFORML %ANAME(対象)%が搾乳されている……

IF TALENT:対象:母乳体質
    SELECTCASE RAND:7
        CASE 0
            PRINTFORML %ANAME(対象)%は畜舎に繋がれている
            PRINTFORML %STR_BODY("胸", 対象)%を搾乳機に吸い上げられ、甘い声をあげてしまう……
        CASE 1
            PRINTFORML %ANAME(対象)%は農家に%STR_BODY("胸：愛撫",対象)%を搾られている
            PRINTFORML プロに揉みしだかれ、%STR_BODY("乳首：欲情",対象)%を弄ばれ、何度も噴乳した……
        CASE 2
            PRINTFORML %ANAME(対象)%は搾乳機を取り付けられ母乳を吸われている
            PRINTFORML 機械の与える無機質な刺激に何度も達してしまった……
        CASE 3
            PRINTFORML %ANAME(対象)%が畜舎に繋がれると、すぐ搾乳が開始された
            PRINTFORML 家畜のような扱いも、%STR_BODY("胸",対象)%を弄ばれる快感の前にはどうでもよくなった……
        CASE 4
            PRINTFORML %ANAME(対象)%は搾乳機を見ただけで身体を熱くしてしまう
            PRINTFORML 本人も知らぬうちに、その%STR_BODY("身体",対象)%は家畜らしく作り替えられている……
        CASE 5
            PRINTFORML 担当の農家は%ANAME(対象)%の%STR_BODY("胸",対象)%を念入りにマッサージする
            PRINTFORML そのおかげか、今日の%ANAME(対象)%はいつもより多く母乳を出したようだ……
        CASE 6
            PRINTFORML 担当の農家が%ANAME(対象)%の%STR_BODY("胸",対象)%の様子や母乳量を詳細に記録していく
            PRINTFORML 己のはしたない記録を残されることに、%ANAME(対象)%は羞恥を感じつつも興奮してしまった……
    ENDSELECT

    CALL FUCK(対象, "Ｂ感, 噴乳, 苦痛快楽, マゾ", "CFLAG減少")

    EXP:対象:搾乳経験値 += (1 + RAND:3)
    SIF TALENT:対象:Ｂ敏感
        EXP:対象:搾乳経験値 += 1
    SIF TALENT:対象:Ｂ鈍感
        EXP:対象:搾乳経験値 += -1
    SIF GETBIT(TALENT:対象:淫乱系, 素質_淫乱_淫乳)
        EXP:対象:搾乳経験値 += 2
    SIF GETBIT(TALENT:対象:淫乱系, 素質_淫乱_雌牛)
        EXP:対象:搾乳経験値 += 2
    SIF ABL:対象:Ｂ感 >= ランク閾値:ランク_その他:ランク_D
        EXP:対象:搾乳経験値 += 2

    IF EXP:対象:搾乳経験値 >= 50 && CFLAG:対象:一時的母乳体質
        CALL COLOR_PRINTL(@"度重なる搾乳により、%ANAME(対象)%の乳腺はすっかりなじんだ", カラー_ピンク)
        CALL COLOR_PRINTL(@"%ANAME(対象)%の＜母乳体質＞は定着した", カラー_ピンク)
        CFLAG:対象:一時的母乳体質 = 0
    ENDIF

    噴乳量 = ABL:対象:噴乳 * 2 + ABL:対象:Ｂ感 * 2 + 3

    SIF TALENT:対象:Ｂ敏感
        噴乳量 *= 3 / 2
    SIF TALENT:対象:Ｂ鈍感
        噴乳量 *= 3 / 4
    SIF GETBIT(TALENT:対象:淫乱系, 素質_淫乱_淫乳)
        噴乳量 *= 3 / 2
    SIF GETBIT(TALENT:対象:淫乱系, 素質_淫乱_雌牛)
        噴乳量 *= 2
    噴乳量 = MAX(噴乳量, 1)
	;調教時より時間が長いから一回100ccと適当に見積もる
    噴乳量 = 噴乳量 * 100
    PRINTFORML %ANAME(対象)%からは{噴乳量}ccの母乳が搾り出せたようだ

ELSE
    SELECTCASE RAND:6
        CASE 0
            PRINTFORML 母乳が出るようになるよう、%ANAME(対象)%は農家からマッサージを受けている
            PRINTFORML プロの手で%STR_BODY("胸",対象)%や%STR_BODY("乳首",対象)%を弄ばれ、何度も甘い声をあげてしまった……
        CASE 1
            PRINTFORML %ANAME(対象)%は搾乳機に繋がれ、%STR_BODY("胸", 対象)%を吸い上げられている
            PRINTFORML 母乳は出ないが、刺激に何度も絶頂してしまった……
        CASE 2
            PRINTFORML 母乳が出るようになるよう、%ANAME(対象)%は搾乳機で%STR_BODY("胸",対象)%を強引に吸われている
            PRINTFORML 家畜のような扱いも、%STR_BODY("胸",対象)%を弄ばれる快感の前にはどうでもよくなった……
        CASE 3
            PRINTFORML %ANAME(対象)%は担当の農家に、入念な母乳マッサージを施されている
            PRINTFORML %STR_BODY("胸", 対象)%や%STR_BODY("乳首", 対象)%を弄ばれる切なさに、空腰を振っている……
        CASE 4
            PRINTFORML %ANAME(対象)%は搾乳機を見ただけで身体を熱くしてしまう
            PRINTFORML 本人も知らぬうちに、その%STR_BODY("身体",対象)%は家畜らしく作り替えられている……
        CASE 5
            PRINTFORML 畜舎に繋がれた%ANAME(対象)%が、担当農家に%STR_BODY("胸",対象)%を弄ばれている
            PRINTFORML 母乳を出すためのマッサージに、甘い声をあげている……
    ENDSELECT
    CALL FUCK(対象, "Ｂ感, 噴乳, 苦痛快楽, マゾ", "CFLAG減少")

    EXP:対象:搾乳経験値 += (1 + RAND:3)
    SIF TALENT:対象:Ｂ敏感
        EXP:対象:搾乳経験値 += 1
    SIF TALENT:対象:Ｂ鈍感
        EXP:対象:搾乳経験値 += -1
    SIF GETBIT(TALENT:対象:淫乱系, 素質_淫乱_淫乳)
        EXP:対象:搾乳経験値 += 2
    SIF GETBIT(TALENT:対象:淫乱系, 素質_淫乱_雌牛)
        EXP:対象:搾乳経験値 += 2
    SIF ABL:対象:Ｂ感 >= ランク閾値:ランク_その他:ランク_D
        EXP:対象:搾乳経験値 += 2

    IF EXP:対象:搾乳経験値 >= 40
        CALL COLOR_PRINTL(@"度重なる搾乳により、%ANAME(対象)%の乳腺から母乳が滲みはじめた", カラー_ピンク)
        CALL COLOR_PRINTL(@"%ANAME(対象)%は＜母乳体質＞を得た", カラー_ピンク)
        TALENT:対象:母乳体質 = 1
    ENDIF
ENDIF

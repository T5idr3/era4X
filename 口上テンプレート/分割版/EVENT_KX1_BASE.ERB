;反発刻印Lv3
IF MARK:3 == 3 && (CFLAG:202 < 4 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 4
;屈服刻印Lv0
ELSEIF MARK:2 == 0 && (CFLAG:202 < 1 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 1
;屈服刻印Lv1
ELSEIF MARK:2 == 1 && (CFLAG:202 < 2 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 2
;屈服刻印Lv2
ELSEIF MARK:2 == 2 && (CFLAG:202 < 3 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 3
;屈服刻印Lv3＋陥落素質無し
ELSEIF MARK:2 == 3 && (TALENT:恋慕 == 0 && TALENT:淫乱 == 0 && TALENT:服従 == 0 && TALENT:親愛 == 0 && TALENT:娼婦 == 0 && TALENT:隷属 == 0) && (CFLAG:202 < 4 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 4
ENDIF
RETURN 0

